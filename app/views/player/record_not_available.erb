  <p>
    The specified record is available to UC Berkeley users only.
  </p>

  <table>
    <thead>
    <tr>
      <th colspan="2"><h3>Details</h3></th>
    </tr>
    </thead>
    <tbody>
    <tr>
      <th>Collection</th>
      <td><p><%= collection %></p></td>
    </tr>
    <tr>
      <th>Record ID</th>
      <td><p><%= record_id %></p></td>
    </tr>
    </tbody>
  </table>

</section>
